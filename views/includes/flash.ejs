
<% if(success &&  success.length) {%>
    <div class="alert alert-success alert-dismissible" role="alert"  style="margin-top: 100px; z-index:2;">
        <%=success %>
        <button type="button" 
        class="btn-close"
        data-bs-dismiss="alert"
        aria-label="close"
        ></button>
    </div>
    <%} %>
<% if(error && error.length) {%>
      <div class="alert alert-danger alert-dismissible fade show col-6 offset-3" role="alert" style="margin-top: 100px; z-index:2">
        <%= error %>
          <button type="button" 
          class="btn-close"
         data-bs-dismiss="alert"
        aria-label="close"
         ></button>
    </div>
 <%} %>
